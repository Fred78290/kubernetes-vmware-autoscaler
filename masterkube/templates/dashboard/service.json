{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "labels": {
            "k8s-app": "kubernetes-dashboard"
        },
        "name": "kubernetes-dashboard",
        "namespace": "$K8NAMESPACE"
    },
    "spec": {
        "type": "NodePort",
        "ports": [
            {
                "port": 443,
                "targetPort": 8443,
                "nodePort": 31443
            }
        ],
        "selector": {
            "k8s-app": "kubernetes-dashboard"
        }
    }
}