{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "labels": {
            "app": "helloworld"
        },
        "name": "helloworld",
        "namespace": "$K8NAMESPACE"
    },
    "spec": {
        "type": "ClusterIP",
        "ports": [
            {
                "port": 8080,
                "targetPort": 8080
            }
        ],
        "selector": {
            "app": "helloworld"
        }
    }
}