{
    "address": "$CONNECTTO",
    "secret": "vmware",
    "timeout": 300,
    "config": {
        "update-etc-hosts": ${HA_CLUSTER},
        "cluster-hostname": "${MASTERKUBE}.${DOMAIN_NAME}",
        "kubeAdmAddress": "$MASTER_IP",
        "kubeAdmToken": "$TOKEN",
        "kubeAdmCACert": "sha256:$CACERT",
        "kubeAdmExtraArguments": [
            "--ignore-preflight-errors=All"
        ]
    }
}