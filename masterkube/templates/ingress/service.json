{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "masterkube-ingress-nginx",
        "namespace": "$K8NAMESPACE"
    },
    "spec": {
        "type": "NodePort",
        "ports": [
            {
                "name": "http",
                "port": 80,
                "targetPort": 80,
                "nodePort": 30080,
                "protocol": "TCP"
            },
            {
                "name": "https",
                "port": 443,
                "targetPort": 443,
                "nodePort": 30443,
                "protocol": "TCP"
            }
        ],
        "selector": {
            "app": "masterkube-ingress-nginx"
        }
    }
}